{
  "name": "painai-timesheet-backend",
  "version": "1.0.0",
  "description": "Backend API for Painai Timesheet Management System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc --skipLibCheck",
  "start": "npx prisma migrate deploy && node dist/index.js",
    "test": "jest",
    "test:all": "ts-node src/prisma/run-all-tests.ts",
    "test:quick": "ts-node src/prisma/run-all-tests.ts quick",
    "test:performance": "ts-node src/prisma/run-all-tests.ts performance",
    "test:security": "ts-node src/prisma/run-all-tests.ts security",
    "test:suite": "ts-node src/prisma/run-all-tests.ts suite",
    "test:help": "ts-node src/prisma/run-all-tests.ts help",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node src/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.9",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "jest": "^29.7.0",
